<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lisa.testcreator.TestCreatorController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Файл">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#loadTestsFromPath" text="Открыть тест" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox fx:id="mainViewHBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="467.0" prefWidth="925.0">
         <children>
            <ListView fx:id="testsListListView" onMouseClicked="#testsItemClicked" prefHeight="409.0" prefWidth="215.0" />
            <VBox fx:id="greetingsVBox" alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" prefHeight="467.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
